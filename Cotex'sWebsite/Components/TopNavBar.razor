<MudAppBar Color="Color.Primary" Fixed="true">
    <MudIconButton Icon="@Icons.Material.Filled.Menu" Color="Color.Inherit" Edge="Edge.Start" OnClick="() => sideMenuOpen = !sideMenuOpen"/>
    <MudSpacer />
    <MudIconButton Icon="@Icons.Material.Filled.Gavel" Color="Color.Inherit" />
    <MudText>Coter Abogados</MudText>
    <MudSpacer/>
</MudAppBar>
<MudDrawer Width="300px" Elevation="25" @bind-Open="sideMenuOpen" ClipMode="DrawerClipMode.Always" Variant="DrawerVariant.Temporary">
    <MudDrawerHeader>
        <MudText Typo="Typo.h5">Estamos para servirle</MudText>
    </MudDrawerHeader>
    <MudNavMenu>
        <MudNavLink Href="@PagesRoutes.PracticaJuridica">Practica Juridica</MudNavLink>
        <MudNavLink Href="@PagesRoutes.QuienesSomos">Quienes Somos</MudNavLink>
        <MudNavLink Href="@PagesRoutes.Contacto">Contacto</MudNavLink>
    </MudNavMenu>
</MudDrawer>